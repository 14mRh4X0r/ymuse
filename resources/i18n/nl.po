msgid ""
"{{- if or .Title .Album | or .Artist -}}\n"
"<big><b>{{ .Title | default \"(unknown title)\" }}</b></big>\n"
"by <b>{{ .Artist | default \"(unknown artist)\" }}</b> from <b>{{ .Album | "
"default \"(unknown album)\" }}</b>\n"
"{{- else if .Name -}}\n"
"<big><b>{{ .Name }}</b></big>\n"
"{{- else if .file -}}\n"
"File <big><b>{{ .file | basename }}</b></big>\n"
"from <b>{{ .file | dirname }}</b>\n"
"{{- else -}}\n"
"<i>(no track)</i>\n"
"{{- end -}}\n"
msgstr ""
"{{- if or .Title .Album | or .Artist -}}\n"
"<big><b>{{ .Title | default \"(onbekende titel)\" }}</b></big>\n"
"door <b>{{ .Artist | default \"(onbekende artiest)\" }}</b> uit <b>{{ .Album "
"| default \"(onbekend album)\" }}</b>\n"
"{{- else if .Name -}}\n"
"<big><b>{{ .Name }}</b></big>\n"
"{{- else if .file -}}\n"
"Bestand <big><b>{{ .file | basename }}</b></big>\n"
"in map <b>{{ .file | dirname }}</b>\n"
"{{- else -}}\n"
"<i>(geen track)</i>\n"
"{{- end -}}\n"

